<% provide(:title, "Edit company") %>


<div class="row">

    <%= form_for @company do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :location %>
        <%=f.text_field :location%>

        <%= f.label :contacts %>
        <%=f.text_field :contacts%>

        <%= f.label :type %>
        <%=f.text_field :ownership%>
      <br>
        <%= f.submit "Save changes", :class=>"button" %>
      </br>
    <% end %>

</div>
