<% provide(:title, "New company") %>

<%= form_for @company do |f| %>
    <%= f.label :name %>
    <%=f.text_field :name%>
    <%= f.label :location %>
    <%=f.text_field :location%>
    <%= f.label :contacts %>
    <%=f.text_field :contacts%>
    <%= f.label :ownership_type %>
    <%=f.text_field :ownership%>

    <br><center><%= f.submit "Submit", :class=>"button" %></center></br>



<% end%>
